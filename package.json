{
  "name": "@djdavid98/chiseltime",
  "version": "0.1.0",
  "description": "An application that helps with automating Discord message updates",
  "author": "DJDavid98 <inbox@djdavid98.art> (https://djdavid98.art)",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:server": "nest build",
    "build:client": "cd src/client && next build",
    "build": "concurrently -p name -g \"npm:build:*\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/client/**/*.tsx\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretypeorm": "npm run build:server",
    "typeorm": "typeorm-ts-node-commonjs",
    "pretypeorm:generate": "npm run build:server",
    "typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d src/server/common/data-source.ts",
    "pretypeorm:run": "npm run build:server",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/server/common/data-source.ts",
    "pretypeorm:revert": "npm run build:server",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/server/common/data-source.ts"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.0",
    "@eropple/nestjs-correlation-id": "^1.0.1",
    "@js-temporal/polyfill": "^0.4.3",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.3.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "discord-api-types": "^0.37.38",
    "joi": "^17.9.1",
    "nestjs-pino": "^3.1.3",
    "next": "^13.3.0",
    "passport": "^0.6.0",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.10.0",
    "pino-pretty": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-topbar-progress-indicator": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@next/eslint-plugin-next": "^13.3.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/passport-oauth2": "^1.4.12",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "sass": "^1.62.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
